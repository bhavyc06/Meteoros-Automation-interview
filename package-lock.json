{
  "name": "my-webapp",
  "scripts": {
    "dev": "concurrently \"npm:start --prefix server\" \"npm:start --prefix client\"",
    "build": "npm --prefix client run build && npm run copy-build",
    "copy-build": "rm -rf server/dist && cp -r client/dist server/dist",
    "start": "node server/src/server.js"
  },
  "devDependencies": {
    "concurrently": "^8.2.2"
  }
}
